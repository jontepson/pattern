    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "expo": "^44.0.0",
    "expo-app-auth": "~11.1.0",
    "expo-auth-session": "~3.5.0",
    "expo-barcode-scanner": "~11.2.0",
    "expo-location": "~14.0.1",
    "expo-random": "~12.1.0",
    "expo-status-bar": "~1.2.0",
    "expo-web-browser": "~10.1.0",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-app-auth": "^6.4.0",
    "react-native-geolocation-service": "^5.3.0-beta.3",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-get-location": "^2.1.2",
    "react-native-location": "^2.5.0",
    "react-native-maps": "0.29.4",
    "react-native-oauth": "^2.1.18",
    "react-native-paper": "^4.4.1",
    "react-native-reanimated": "~2.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "12.1.1",
    "react-native-web": "0.17.1",
    "jest": "^26.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "babel-eslint": "^10.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.27.1",
    "jest-expo": "^44.0.1",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@fortawesome)"
    ],
    "coveragePathIgnorePatterns": [
      "src/screens/LoggedInScreen.js",
      "src/screens/MapScreen.js",
      ".eslintrc.js",
      "src/hooks/logDataHook.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
